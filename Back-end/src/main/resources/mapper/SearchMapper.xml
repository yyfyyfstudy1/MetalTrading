<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.usyd.capstone.mapper.SearchMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.usyd.capstone.entity.Search">
        <id column="search_id" property="searchId" />
        <result column="user_id" property="userId" />
        <result column="search_content" property="searchContent" />
        <result column="search_time" property="searchTime" />
        <result column="product_id" property="productId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        search_id, user_id, search_content, search_time, product_id
    </sql>

</mapper>
